<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="55dp">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/user_image_profile"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/no_image"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/user_name_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="TextView"
        android:textColor="#FFF"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/user_image_profile"
        app:layout_constraintTop_toBottomOf="@+id/user_image_profile" />

    <TextView
        android:id="@+id/year_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="2nd"
        android:textColor="#FFF"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="@+id/imageView13" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="6dp"
        android:text="@string/year"
        android:textColor="#FFF"
        app:layout_constraintStart_toEndOf="@+id/year_profile"
        app:layout_constraintTop_toTopOf="@+id/year_profile" />

    <TextView
        android:id="@+id/branch_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="6dp"
        android:text="IT"
        android:textColor="#FFF"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/college_img_view_profile"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="14dp"
        android:src="#FFF"
        app:layout_constraintStart_toStartOf="@+id/user_name_profile"
        app:layout_constraintTop_toBottomOf="@+id/user_name_profile" />

    <TextView
        android:id="@+id/college_name_text_view_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:text="TextView"
        android:textColor="#FFF"
        app:layout_constraintStart_toEndOf="@+id/college_img_view_profile"
        app:layout_constraintTop_toTopOf="@+id/college_img_view_profile" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView13"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="36dp"
        app:layout_constraintStart_toEndOf="@+id/college_name_text_view_profile"
        app:layout_constraintTop_toTopOf="@+id/college_name_text_view_profile"
        android:src="#FFF" />

<!--    <com.facebook.drawee.view.SimpleDraweeView-->
<!--        android:id="@+id/imageview_restaurant"-->
<!--        android:layout_width="139dp"-->
<!--        android:layout_height="119dp"-->
<!--        app:layout_constraintBottom_toBottomOf="parent"-->
<!--        app:layout_constraintEnd_toEndOf="parent"-->
<!--        app:layout_constraintStart_toStartOf="parent"-->
